[user]
  name =
  email =
[core]
  editor = vim -w
  excludesfile = $HOME/.gitignore_global
[commit]
  template = $HOME/gitmessage.txt
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red
  new = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[diff]
  renames = copies
[help]
  autocorrect = 1

